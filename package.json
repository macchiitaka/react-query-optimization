{
  "name": "react-query-optimization",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "fmt": "run-s fmt:prettier fmt:package-json fmt:eslint",
    "fmt:prettier": "prettier --write --tab-width 2 \"**/*.{js,jsx,ts,tsx,json,yml,yaml,md}\"",
    "fmt:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache --cache-strategy content --report-unused-disable-directives --fix",
    "fmt:package-json": "sort-package-json",
    "lint": "run-p lint:**",
    "lint:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache --cache-strategy content",
    "lint:type-check": "tsc -p . --noEmit"
  },
  "dependencies": {
    "next": "12.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.34.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.2.0",
    "@takamachi/eslint-config": "^2.3.0",
    "@takamachi/prettier-config": "^1.1.0",
    "@types/node": "16.11.11",
    "@types/react": "17.0.37",
    "eslint": "8.4.0",
    "eslint-config-next": "12.0.7",
    "npm-run-all": "4.1.5",
    "prettier": "^2.5.1",
    "sort-package-json": "^1.53.1",
    "typescript": "4.5.2"
  }
}
